{
  "name": "tailpieces",
  "version": "2.0.2-alpha.21",
  "description": "Vue 3 + TypeScript + TailwindCss Components Library UI",
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "main": "dist/tailpieces.ssr.js",
  "module": "dist/tailpieces.esm.js",
  "browser": "dist/tailpieces.esm.js",
  "unpkg": "dist/tailpieces.min.js",
  "files": [
    "dist/*",
    "tailpieces.d.ts",
    "src/**/*.vue"
  ],
  "dependencies": {
    "@vueup/vue-quill": "^1.0.0-alpha.39",
    "npm": "^8.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.4",
    "@vue/cli-plugin-eslint": "~5.0.4",
    "@vue/cli-plugin-typescript": "~5.0.4",
    "@vue/cli-service": "~5.0.4",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "autoprefixer": "^10.4.5",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "minimist": "^1.2.5",
    "postcss": "^8.4.12",
    "prettier": "^2.4.1",
    "rollup": "^2.36.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "tailwindcss": "^3.0.24",
    "typescript": "~4.5.5",
    "vue": "^3.2.33"
  },
  "peerDependencies": {
    "vue": "^3.0.5"
  },
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "tailwindcss",
    "vue",
    "vue3",
    "components",
    "ui framework",
    "component framework",
    "ui library",
    "component library",
    "forms",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlanRezende/tailpieces.git"
  },
  "sideEffects": false,
  "types": "tailpieces.d.ts"
}
